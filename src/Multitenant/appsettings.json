{
    "ConnectionStrings": {
        "DefaultConnection": "DataSource=db\\default.db",
        "ConnectionStringTemplate": "DataSource={tenant}"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "DefaultTenantUrl": "https://default.tenancy.localhost:5001",
    "AllowedHosts": "*",
    "Kestrel": {
        "EndPoints": {
            "Http": {
                "Url": "http://localhost:5000"
            },

            "Default": {
                "Url": "https://localhost:5001"
            }
        }
    }
}
